{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["**/*.ts", "src/**/*.ts"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/main.ts"
      }
    ]
  }
  